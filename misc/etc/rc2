date

# Make nodes
mknod /dev/null c 3 0
mknod /dev/random c 4 0
cat /dev/null >/etc/utmp
touch /etc/mount /dev/rp3
echo 'clearing /tmp'
rm -rf /tmp/
mkdir /tmp

echo 'updating motd'
echo -n "Welcome to FreeNIX " > /etc/motd
uname -v >> /etc/motd

echo -n 'starting daemons:'
echo -n ' cron';	cron
echo '.'

sh /etc/rc.local
date; echo
